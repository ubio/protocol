        "AvailableQuestionSelectOne": {
            "type": "object",
            "properties": {
                "inputType": { "enum": [ "SelectOne" ] },
                "id": {
                    "type": "string"
                },
                "label": {
                    "type": "string"
                },
                "question": {
                    "type": "string"
                },
                "mandatory": {
                    "type": "boolean"
                }
            },
            "required": [
                "inputType",
                "id",
                "label",
