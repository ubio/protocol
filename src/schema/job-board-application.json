                "mandatory"
            ],
            "additionalProperties": false
        },
        "AvailableQuestionSelectMultiple": {
            "type": "object",
            "properties": {
                "inputType": { "enum": [ "SelectMultiple" ] },
                "id": {
                    "type": "string"
                },
                "label": {
                    "type": "string"
                },
                "question": {
                    "type": "string"
                },
                "mandatory": {
                    "type": "boolean"
                }
            },
            "required": [
                "inputType",
                "id",
                "label",
                "question",
                "mandatory"
            ],
            "additionalProperties": false
        },
        "AvailableQuestionBoolean": {
            "type": "object",
            "properties": {
                "inputType": { "enum": [ "Boolean" ] },
                "id": {
