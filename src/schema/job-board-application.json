                    "type": "string"
                },
                "value": {
                    "type": "array",
                    "items": {
                        "$ref": "#/domains/JobBoardApplication/types/SelectedDynamicQuestions"
                    }
                },
                "mandatory": {
                    "type": "boolean"
                }
            },
            "required": [
                "id",
                "value"
            ],
            "additionalProperties": false
        },
        "SelectedQuestionFile": {
            "type": "object",
            "properties": {
                "inputType": { "enum": [ "File" ] },
                "id": {
                    "type":"string"
                },
                "label": {
                    "type": "string"
                },
                "question": {
                    "type":"string"
                },
                "value": {
                    "typeRef": "#/domains/Generic/types/File"

