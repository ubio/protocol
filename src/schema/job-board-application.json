            ],
            "additionalProperties": false
        },
        "AvailableQuestionArrayOfObjects": {
            "type": "object",
            "properties": {
                "inputType": { "enum": [ "ArrayOfObjects" ] },
                "id": {
                    "type": "string"
                },
                "label": {
                    "type": "string"
