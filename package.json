{
  "name": "@ubio/protocol",
  "version": "5.3.2",
  "description": "ubio Automation Protocol",
  "main": "src/index.js",
  "author": "ubio",
  "license": "MIT",
  "files": [
    "src/"
  ],
  "browser": {
    "node-fetch": "./src/fetch-browser.js"
  },
  "scripts": {
    "check": "eslint --ext=js,vue .",
    "dev": "run-p watch:* serve",
    "build": "rimraf public && npm run build:copy-static && npm run build:js && npm run build:schema && npm run build:html-cache",
    "serve": "serve public",
    "build:copy-static": "node ./build-copy-static.js",
    "build:js": "browserify site/ -o public/app.js",
    "build:schema": "node ./build-schema.js",
    "build:html-cache": "node ./build-html-cache.js",
    "watch:js": "watchify -p browserify-hmr site/ -o public/app.js",
    "test": "mocha",
    "preversion": "run-s check test",
    "version": "run-s build",
    "postversion": "npm publish --access public && git push origin master --tags"
  },
  "pre-commit": [
    "check"
  ],
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babelify": "^8.0.0",
    "browserify": "^14.5.0",
    "browserify-hmr": "^0.3.5",
    "eslint": "^4.10.0",
    "eslint-config-ub": "^2.0.0",
    "eslint-plugin-vue": "^2.1.0",
    "mocha": "^4.0.1",
    "npm-run-all": "^4.1.2",
    "pre-commit": "^1.2.2",
    "rimraf": "^2.6.2",
    "serve": "^10.0.1",
    "vue": "^2.5.3",
    "vue-router": "^3.0.1",
    "vueify": "^9.4.1",
    "watchify": "^3.9.0"
  },
  "dependencies": {
    "ajv": "5.3.0",
    "jsonpointer": "^4.0.1",
    "node-fetch": "1.7.3"
  }
}
